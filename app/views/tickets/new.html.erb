<%= provide(:title, 'New Ticket') %>
<h1>New Ticket</h1>

<div class="row">
  <div class="span6 offset3">
    <%= form_for(@ticket) do |f| %>
        <%= render 'fields', f: f %>
        <div>
          <%= f.submit "Create", class: "btn btn-large btn-primary" %>
        </div>
    <% end %>
  </div>
</div>